{
  "name": "nbr.haus",
  "private": true,
  "type": "module",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "serve": "vite preview",
    "serve:host": "npm run build && vite preview --host --port 3000",
    "test": "vitest run",
    "check:format": "prettier --check .",
    "format": "prettier --write .",
    "check:lint": "eslint .",
    "lint": "eslint --fix .",
    "lint:inspect": "eslint --inspect-config",
    "check:compile": "tsc --noEmit",
    "check": "npm run check:format && npm run check:lint && npm run check:compile && npm run knip",
    "knip": "knip",
    "storybook": "storybook dev -p 6006 -c src/storybook",
    "build:storybook": "storybook build -c src/storybook",
    "generate:cv": "npx tsx scripts/generate-cv-pdf.ts",
    "generate:coverletters": "npx tsx scripts/generate-coverletters-pdf.ts",
    "docs:ai:ts-router": "rm -f docs/ai/tanstack-router.md && vibe-rules install claude-code @tanstack/react-router --target docs/ai/tanstack-router.md",
    "postinstall": "npx playwright install chromium"
  },
  "dependencies": {
    "@tanstack/react-devtools": "0.9.5",
    "@tanstack/react-router": "^1.144.0",
    "@tanstack/react-router-devtools": "^1.144.0",
    "@tanstack/react-router-ssr-query": "^1.144.0",
    "@tanstack/react-start": "^1.145.0",
    "@tanstack/router-plugin": "^1.144.0",
    "class-variance-authority": "^0.7.1",
    "gray-matter": "^4.0.3",
    "lucide-react": "0.564.0",
    "motion": "^12.23.26",
    "nitro": "3.0.1-alpha.1",
    "open-props": "1.7.23",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-markdown": "^10.1.0",
    "rehype-slug": "^6.0.0",
    "shiki": "^3.20.0",
    "vite-tsconfig-paths": "6.1.1",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^5.0.1",
    "@eslint/compat": "2.0.2",
    "@eslint/css": "0.14.1",
    "@eslint/js": "9.39.1",
    "@eslint/json": "0.14.0",
    "@eslint/markdown": "7.5.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@storybook/addon-a11y": "^10.0.8",
    "@storybook/addon-docs": "^10.0.8",
    "@storybook/addon-onboarding": "^10.0.8",
    "@storybook/addon-vitest": "^10.0.8",
    "@storybook/react-vite": "^10.0.8",
    "@tanstack/devtools-vite": "^0.4.0",
    "@testing-library/dom": "10.4.1",
    "@testing-library/react": "16.3.2",
    "@types/node": "^25.2.3",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.4",
    "@vitest/browser": "4.0.18",
    "@vitest/coverage-v8": "4.0.18",
    "eslint": "9.39.1",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-storybook": "^10.0.8",
    "globals": "17.3.0",
    "h3": "^2.0.1-rc.6",
    "jsdom": "28.1.0",
